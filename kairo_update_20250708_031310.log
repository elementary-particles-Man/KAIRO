[workspace]
members = [
    "rust-core"
]
resolver = "2"   # Rust 2021 Editionに合わせて resolver を2に固定

# workspace.dependencies は無理に使わず、基本は個別で管理する
# 必要なら以下のように共通依存を設定する例
# [workspace.dependencies]
# criterion = "0.5"

# ========================================
# rust-core/Cargo.toml (Member)
# ========================================

[package]
name = "rust-core"
version = "0.1.0"
edition = "2021"

[dependencies]
# === Rand 系は1系統に統一 ===
rand = "0.8"
rand_core = "0.6"
rand_chacha = "0.3"

# === 暗号系 ===
curve25519-dalek = "4.1"
ed25519-dalek = "1.0"

# === FlatBuffers ===
flatbuffers = "25.2"

# === Libloading ===
libloading = "0.7"

# === Chronoなど日付系 ===
chrono = { version = "0.4", features = ["serde"] }

# === Logging ===
log = "0.4"

# === その他必要な汎用ユーティリティ ===
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"

# === テスト系 (必要に応じて) ===
criterion = { version = "0.5", optional = true }
mockall = { version = "0.12", optional = true }
rstest = { version = "0.18", optional = true }

[dev-dependencies]
# dev系はここに
criterion = "0.5"
mockall = "0.12"
rstest = "0.18"

[features]
# 必要に応じて feature を切る
default = []

# ========================================
# 追加推奨
# ========================================
# - 必要なら [patch.crates-io] で依存バージョンを上書き
# - 必要なら [profile.dev] / [profile.release] で最適化を調整


